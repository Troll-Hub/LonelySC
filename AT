local _=string.char
local __=game.GetService
local ___=game

local a=__(___,_(80,108,97,121,101,114,115))
local b=__(___,_(82,101,112,108,105,99,97,116,101,100,83,116,111,114,97,103,101))

local c=a.LocalPlayer
local d=b._BindableEvents.Moves.TakeDamage

task.spawn(function()
	while true do
		task.wait(1)

		local e=c.Character
		if not e then continue end

		local f=e:FindFirstChildOfClass(_(72,117,109,97,110,111,105,100))
		if f then
			d:FireServer(f,1e11,c)
		end
	end
end)
